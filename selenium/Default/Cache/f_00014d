    <script>
      
      
      var mainJS = '/assets/application-df4f823cc80fa4dc5368d51a2f3dadbc.js';

      var resources = [
        mainJS,
        "/assets/libs/utils-04ad04e99b80015037821c85d81f4433.js",
      ];
      resources.forEach(function(source) {
        var script = document.createElement('script');
